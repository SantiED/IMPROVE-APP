classdef STARTUP2 < matlab.apps.AppBase

    % Properties that correspond to app components
    properties (Access = public)
        UIFigure       matlab.ui.Figure
        IntegrantesJesusAlejandroCordobaOrtizLabel  matlab.ui.control.Label
        CienciaytcnicaconhumanismoLabel  matlab.ui.control.Label
        Label_3        matlab.ui.control.Label
        Label_2        matlab.ui.control.Label
        Image          matlab.ui.control.Image
        IMPROVEAPPMejoratusimagenespreferidasLabel  matlab.ui.control.Label
        IniciarButton  matlab.ui.control.Button
    end

    
    methods (Access = private)
        
    end
    

    % Callbacks that handle component events
    methods (Access = private)

        % Button pushed function: IniciarButton
        function IniciarButtonPushed(app, event)
        global Imagen
            %clear Imagen
        close(app.UIFigure);     
        MAIN2(Imagen)
        
        
        end
    end

    % Component initialization
    methods (Access = private)

        % Create UIFigure and components
        function createComponents(app)

            % Create UIFigure and hide until all components are created
            app.UIFigure = uifigure('Visible', 'off');
            app.UIFigure.Color = [0.302 0.7451 0.9333];
            app.UIFigure.Position = [100 100 635 445];
            app.UIFigure.Name = 'MATLAB App';
            app.UIFigure.Pointer = 'hand';

            % Create IniciarButton
            app.IniciarButton = uibutton(app.UIFigure, 'push');
            app.IniciarButton.ButtonPushedFcn = createCallbackFcn(app, @IniciarButtonPushed, true);
            app.IniciarButton.BackgroundColor = [0 0.4471 0.7412];
            app.IniciarButton.FontName = 'Berlin Sans FB';
            app.IniciarButton.FontSize = 20;
            app.IniciarButton.FontColor = [1 1 1];
            app.IniciarButton.Position = [163 50 274 44];
            app.IniciarButton.Text = 'Iniciar';

            % Create IMPROVEAPPMejoratusimagenespreferidasLabel
            app.IMPROVEAPPMejoratusimagenespreferidasLabel = uilabel(app.UIFigure);
            app.IMPROVEAPPMejoratusimagenespreferidasLabel.BackgroundColor = [0 0.4471 0.7412];
            app.IMPROVEAPPMejoratusimagenespreferidasLabel.HorizontalAlignment = 'center';
            app.IMPROVEAPPMejoratusimagenespreferidasLabel.FontName = 'Berlin Sans FB';
            app.IMPROVEAPPMejoratusimagenespreferidasLabel.FontSize = 19;
            app.IMPROVEAPPMejoratusimagenespreferidasLabel.FontColor = [1 1 1];
            app.IMPROVEAPPMejoratusimagenespreferidasLabel.Position = [1 128 635 318];
            app.IMPROVEAPPMejoratusimagenespreferidasLabel.Text = {'IMPROVE APP'; ''; 'Mejora tus imágenes preferidas implementando técnicas de procesamiento'; 'de imágenes con un solo botón, prueba y clasifica con Googlenet '};

            % Create Image
            app.Image = uiimage(app.UIFigure);
            app.Image.Position = [-33 312 202 134];
            app.Image.ImageSource = 'UNITEC IMG.jpg';

            % Create Label_2
            app.Label_2 = uilabel(app.UIFigure);
            app.Label_2.BackgroundColor = [0.0745 0.6235 1];
            app.Label_2.Position = [140 143 357 10];
            app.Label_2.Text = '';

            % Create Label_3
            app.Label_3 = uilabel(app.UIFigure);
            app.Label_3.BackgroundColor = [0.0745 0.6235 1];
            app.Label_3.Position = [153 418 357 10];
            app.Label_3.Text = '';

            % Create CienciaytcnicaconhumanismoLabel
            app.CienciaytcnicaconhumanismoLabel = uilabel(app.UIFigure);
            app.CienciaytcnicaconhumanismoLabel.FontName = 'Berlin Sans FB';
            app.CienciaytcnicaconhumanismoLabel.FontSize = 18;
            app.CienciaytcnicaconhumanismoLabel.FontAngle = 'italic';
            app.CienciaytcnicaconhumanismoLabel.FontColor = [1 1 1];
            app.CienciaytcnicaconhumanismoLabel.Position = [141 384 287 35];
            app.CienciaytcnicaconhumanismoLabel.Text = '-Ciencia y técnica con humanismo.';

            % Create IntegrantesJesusAlejandroCordobaOrtizLabel
            app.IntegrantesJesusAlejandroCordobaOrtizLabel = uilabel(app.UIFigure);
            app.IntegrantesJesusAlejandroCordobaOrtizLabel.HorizontalAlignment = 'center';
            app.IntegrantesJesusAlejandroCordobaOrtizLabel.FontName = 'Berlin Sans FB';
            app.IntegrantesJesusAlejandroCordobaOrtizLabel.FontSize = 14;
            app.IntegrantesJesusAlejandroCordobaOrtizLabel.FontAngle = 'italic';
            app.IntegrantesJesusAlejandroCordobaOrtizLabel.FontColor = [1 1 1];
            app.IntegrantesJesusAlejandroCordobaOrtizLabel.Position = [200 161 238 80];
            app.IntegrantesJesusAlejandroCordobaOrtizLabel.Text = {'Integrantes:'; ''; 'Jesus Alejandro Cordoba Ortiz.'; 'Santiago España Domínguez.'; 'Martin Jovany Almonte Santoyo.'};

            % Show the figure after all components are created
            app.UIFigure.Visible = 'on';
        end
    end

    % App creation and deletion
    methods (Access = public)

        % Construct app
        function app = STARTUP2

            % Create UIFigure and components
            createComponents(app)

            % Register the app with App Designer
            registerApp(app, app.UIFigure)

            if nargout == 0
                clear app
            end
        end

        % Code that executes before app deletion
        function delete(app)

            % Delete UIFigure when app is deleted
            delete(app.UIFigure)
        end
    end
end
